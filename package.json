{
  "private": true,
  "scripts": {
    "pixboplayer-build-css": "node-sass resources/assets/sass/PixboPlayer.scss public/css/PixboPlayer.css",
    "pixboplayer-build-js": "cross-env NODE_ENV=production browserify -g envify -e resources/assets/js/PixboPlayer/PixboPlayer.js -o public/js/PixboPlayer.js",
    "pixboplayer-watch-css": "node-sass -w resources/assets/sass/PixboPlayer.scss public/css/PixboPlayer.css",
    "pixboplayer-watch-js": "watchify -vd -e resources/assets/js/PixboPlayer/PixboPlayer.js -o public/js/PixboPlayer.js",
    "ui-watch-css": "node-sass -w resources/assets/sass/app.scss public/css/app.css",
    "ui-watch-js": "watchify -vd -p browserify-hmr -e resources/assets/js/index.js -o public/js/index.js",
    "ui-build-css": "node-sass resources/assets/sass/app.scss public/css/app.css",
    "ui-build-js": "cross-env NODE_ENV=production browserify -g envify -e resources/assets/js/index.js | uglifyjs --compress warnings=false --mangle -o public/js/index.js",
    "copy-fonts": "ncp resources/assets/fonts public/fonts",
    "watch": "npm-run-all --parallel ui-watch-css ui-watch-js",
    "build": "npm-run-all --parallel ui-build-css ui-build-js",
    "build-pixboplayer": "npm-run-all --parallel pixboplayer-build-css pixboplayer-build-js",
    "fixer": "replace \"module.exports = factory\" \"factory\" \"node_modules/eonasdan-bootstrap-datetimepicker/src/js/bootstrap-datetimepicker.js\"",
    "install": "npm-run-all --parallel copy-fonts build build-pixboplayer"
  },
  "browserify": {
    "transform": [
      "vueify",
      "babelify"
    ]
  },
  "dependencies": {
    "babel-core": "^6.17.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-runtime": "^6.2.0",
    "babelify": "^7.3.0",
    "bootstrap-sass": "^3.3.7",
    "bootstrap-select": "^1.11.2",
    "browserify": "^13.1.1",
    "browserify-hmr": "^0.3.5",
    "cross-env": "^3.1.3",
    "dropzone": "^4.3.0",
    "envify": "^3.4.1",
    "eonasdan-bootstrap-datetimepicker": "^4.17.43",
    "font-awesome": "^4.7.0",
    "jquery": "^3.1.1",
    "moment": "^2.13.0",
    "ncp": "^2.0.0",
    "node-sass": "^3.10.1",
    "npm-run-all": "^3.1.1",
    "replace": "^0.3.0",
    "rest": "^1.3.1",
    "sweetalert": "^1.1.3",
    "uglify-js": "^2.8.15",
    "vegas": "^2.3.1",
    "vue": "^1.0.21",
    "vue-form": "^0.3.1",
    "vue-router": "^0.7.7",
    "vue-strap": "^1.1.36",
    "vue-toast": "^2.0.2",
    "vueify": "^8.7.0",
    "watchify": "^3.7.0"
  }
}
